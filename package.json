{
  "name": "aws-sam-local-helper-commands",
  "description": "A CLI tool to make certain common AWS SAM commands easier to use",
  "version": "0.0.1",
  "author": "Eric Allam",
  "bin": {
    "sam-helper": "./bin/run"
  },
  "bugs": "https://github.com/solve-hq/aws-sam-local-helper-commands/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "inquirer": "^6.2.1",
    "cli-ux": "^4.9.3",
    "aws-sdk": "^2.375.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/solve-hq/aws-sam-local-helper-commands",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "sam-helper",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/solve-hq/aws-sam-local-helper-commands",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
